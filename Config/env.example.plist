<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

    <!-- Environment -->
    <key>ENVIRONMENT</key>
    <string>development</string>

    <!-- Core Gateway -->
    <key>CORE_BASE_URL</key>
    <string>https://core.webkurier.example</string>

    <!-- PhoneCore -->
    <key>PHONECORE_BASE_URL</key>
    <string>https://phonecore.webkurier.example</string>

    <!-- WebSocket -->
    <key>WEBSOCKET_BASE_URL</key>
    <string>wss://ws.webkurier.example</string>

    <!-- Feature Flags -->
    <key>ENABLE_WEBRTC</key>
    <true/>
    <key>ENABLE_DREAMMAKER</key>
    <true/>
    <key>ENABLE_WALLET</key>
    <true/>

</dict>
</plist>